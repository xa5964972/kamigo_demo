<a href="https://liffsdk.line-scdn.net/scanqr/index.html?code=KDjRF6ep2VzwPbA9L7m9&liffClientId=1656359117&state=emXxwX5iJ5AB&liffRedirectUri=https%3A%2F%2Fliffsdk.line-scdn.net%2Fscanqr%2Findex.html" target="_blank">Fooish 技術教學</a>
<body>
  <div id="container">
    <h3 id="result">Something is going to happen here...</h3>
    <button id='123' onclick="scanCodeV3()">QR Code Reader</button>
  </div>
  <script src="https://static.line-scdn.net/liff/edge/2/sdk.js"></script>
  <script>
   
    async function scanCodeV3() {
      await liff.init({ liffId: '1656609726-7anN2L1G' });
      
        const result = await liff.scanCodeV2();
        aab = result.value;
            
         liff.sendMessages([
        {
          type: 'text',
          text: aab
        }
      ])
        .then(() => {
          console.log('message sent');
        })
        .catch((err) => {
          console.log('error', err);
        });
      
    }
     window.onload = scanCodeV3;
   
     setTimeout(function() {
	// IE
	if(document.all) {
		document.getElementById("clickMe").click();
	}
	// 其它瀏覽器
	else {
		var e = document.createEvent("MouseEvents");
		e.initEvent("click", true, true);
		document.getElementById("123").dispatchEvent(e);
	}
}, 2000);
    
  </script><script src="https://static.line-scdn.net/liff/edge/2/non-ios-extensions.js" type="text/javascript"></script>


</body></html>
